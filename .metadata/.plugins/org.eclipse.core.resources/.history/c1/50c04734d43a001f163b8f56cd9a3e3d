import java.util.Locale;
import java.util.Scanner;

public class Main {
	  public static void main(String[] args) {
	        // Pesos das notas
	        final double PESO_A = 3.5;
	        final double PESO_B = 7.5;

	        // Scanner para entrada de dados
	        Scanner scanner = new Scanner(System.in);

	        // Entrada dos valores das notas A e B
	        double notaA = scanner.nextDouble();
	        double notaB = scanner.nextDouble();

	        // Cálculo da média ponderada
	        double media = (notaA * PESO_A + notaB * PESO_B) / (PESO_A + PESO_B);

	        // Exibição do resultado com 5 casas decimais
	        System.out.printf("MEDIA = %.5f\n", media);
	        
	        // Fechando o scanner
	        scanner.close();
	    }
}
