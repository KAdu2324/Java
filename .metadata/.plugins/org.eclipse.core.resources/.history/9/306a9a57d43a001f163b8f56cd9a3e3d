import java.util.Locale;
import java.util.Scanner;

public class Main {
	  public static void main(String[] args) {
		  
	        final double PESO_A = 3.5;
	        final double PESO_B = 7.5;
	        final double PESO_C = 7.5;
	        
	        Locale.setDefault(Locale.US);

	        Scanner scanner = new Scanner(System.in);

	        double notaA = scanner.nextDouble();
	        double notaB = scanner.nextDouble();
	        double notaC = scanner.nextDouble();

	        double media = (notaA * PESO_A + notaB * PESO_B + notaC * PESO_C) / (PESO_A + PESO_B + PESO_C);

	        System.out.printf("MEDIA = %.5f\n", media);
	        
	        scanner.close();
	    }
}
